package Myworkspace;

public class Palind {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="mdm";
		int k=0;
		boolean f=false;
		for (int i=s.length()-1;i>=0;i--)
		{
			if(s.charAt(i)!=s.charAt(k))
			{
				
				f=true;
				break;
			
			}
			
			k++;
		}
if(!f)
	System.out.println("palindrom");
		else
			print("no");


	}


}
